<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="parallel Suit" parallel="tests" thread-count="2">

  <test name="chrome Test">
    <parameter name="browserName" value="chrome"></parameter>

    <classes>
      <class name="com.nopcommerce.demo.testsuite.LoginPageTestWithParameterAndParallel"></class>
    </classes>
  </test>
  <test name="Edge Test">
    <parameter name="browserName" value="edge"></parameter>

    <classes>
      <class name="com.nopcommerce.demo.testsuite.LoginPageTestWithParameterAndParallel"></class>
    </classes>
  </test>
</suite>
